#!ipxe
###
### netboot.xyz-custom menu
###

:start
echo Hello world!
sleep 10
goto custom_exit

:custom_exit
imgfree

kernel https://releases.rancher.com/harvester/master/harvester-master-vmlinuz-amd64 ip=dhcp rd.cos.disable rd.noverifyssl net.ifnames=1 root=live:https://releases.rancher.com/harvester/master/harvester-master-rootfs-amd64.squashfs console=tty1 harvester.install.automatic=true
initrd https://releases.rancher.com/harvester/master/harvester-master-initrd-amd64
boot

exit 0
